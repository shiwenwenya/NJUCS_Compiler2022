int i;

int add(float fvec1[10], float fvec2[10], float res_fvec[10]) {
  int i = 0;
  while (i < 10) {
    res_fvec[i] = fvec1[i] + fvec2[i];
    i = i + 1;
  }
  return 0;
}

int sub(float fvec1[10], float fvec2[10], float res_fvec[10]) {
  int i = 0;
  while (i < 10) {
    res_fvec[i] = fvec1[i] - fvec2[i];
    i = i + 1;
  }
  return 0;
}

int mul(float fvec1[10], float fvec2[10], float res_fvec[10]) {
  int i = 0;
  while (i < 10) {
    res_fvec[i] = fvec1[i] * fvec2[i];
    i = i + 1;
  }
  return 0;
}

int main() {
  float fvec_1[10];
  float fvec_2[10];
  float fvec[10];
  mul(fvec_1, fvec_2, fvec);
  sub(fvec, fvec_1, fvec);
  add(fvec, fvec_2, fvec);
}